{
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"printWidth": 80,
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"importOrder": ["^\\.\\./", "^\\./"],
	"importOrderSortSpecifiers": true
}
